<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.input.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.Controllers.Controller">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="upload" mnemonicParsing="false" onAction="#onUpload" text="Upload">
                            <accelerator>
                                <KeyCodeCombination alt="UP" code="U" control="UP" meta="UP" shift="DOWN" shortcut="UP" />
                            </accelerator>
                        </MenuItem>
                    </items>
                </Menu>
            <Menu mnemonicParsing="false" text="Session">
              <items>
                <MenuItem fx:id="loginItem" mnemonicParsing="false" onAction="#onLogin" text="Login" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem fx:id="registerItem" mnemonicParsing="false" onAction="#onRegister" text="Register" />
              </items>
            </Menu>
            </menus>
        </MenuBar>
        <BorderPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <bottom>
                <VBox prefHeight="58.0" prefWidth="640.0" BorderPane.alignment="CENTER">
                    <children>
                        <VBox prefHeight="8.0" prefWidth="568.0" VBox.vgrow="NEVER">
                            <children>
                                <HBox prefHeight="8.0" prefWidth="568.0" VBox.vgrow="NEVER">
                                    <children>
                                        <Slider fx:id="timeSlider" onDragDone="#onDragDone" HBox.hgrow="ALWAYS">
                                            <padding>
                                                <Insets left="10.0" right="10.0" />
                                            </padding>
                                        </Slider>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                        <HBox prefHeight="45.0" prefWidth="568.0" VBox.vgrow="NEVER">
                            <children>
                                <Button fx:id="playButton" mnemonicParsing="false" onAction="#onPlay" text="Play" HBox.hgrow="ALWAYS">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="pauseButton" mnemonicParsing="false" onAction="#onPause" text="Pause" HBox.hgrow="ALWAYS">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="stopButton" mnemonicParsing="false" onAction="#onStop" text="Stop" HBox.hgrow="ALWAYS">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </bottom>
        </BorderPane>
    </children>
</VBox>
